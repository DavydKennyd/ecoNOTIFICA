{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: \"LoginForm\",\n  data() {\n    return {\n      email: '',\n      password: ''\n    };\n  },\n  methods: {\n    async login() {\n      try {\n        const response = await axios.post('http://localhost:5000/api/auth/login', {\n          email: this.email,\n          password: this.password\n        });\n        const {\n          token\n        } = response.data;\n\n        // Armazena o token no localStorage\n        localStorage.setItem('authToken', token);\n\n        // Redireciona para a página de perfil\n        this.$router.push('/perfil');\n      } catch (error) {\n        console.error(error.response?.data?.message || 'Erro ao realizar login');\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","email","password","methods","login","response","post","token","localStorage","setItem","$router","push","error","console","message"],"sources":["/home/davyd-kennyd/econotifica_projeto_certo/front-end/src/views/LoginForm.vue"],"sourcesContent":["<template>\n\n  <header class=\"header\">\n    <img :src=\"require('@/assets/setima_logo.png')\" alt=\"LOGO-ECONOTIFICA\" class=\"logo\">\n  </header>\n  <div id=\"app\" class=\"login-container\">\n    <div class=\"overlay\"></div>\n    <div class=\"content\">\n      <img :src=\"require('@/assets/image_planet.png')\" alt=\"Earth\" class=\"globe-img\" />\n      <form @submit.prevent=\"login\" class=\"login-form\">\n        <h2>Login</h2>\n        <div class=\"form-group\">\n          <label for=\"email\">Email:</label>\n          <input type=\"email\" v-model=\"email\" id=\"email\" placeholder=\"exemplo@gmail.com\" required />\n        </div>\n        <div class=\"form-group\">\n          <label for=\"password\">Senha:</label>\n          <input type=\"password\" v-model=\"password\" id=\"password\" placeholder=\"******\" required />\n        </div>\n        <a href=\"#\" class=\"forgot-password\">Esqueceu a senha?</a>\n        <center><button type=\"submit\" class=\"btn-login botao--\">ENTRAR</button></center>\n      </form>\n    </div>\n  </div>\n</template>\n\n\n\n\n/*Exportação*/\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: \"LoginForm\",\n  data() {\n    return {\n      email: '',\n      password: '',\n    };\n  },\n  methods: {\n  async login() {\n    try {\n      const response = await axios.post('http://localhost:5000/api/auth/login', {\n        email: this.email,\n        password: this.password,\n      });\n\n      const { token } = response.data;\n\n      // Armazena o token no localStorage\n      localStorage.setItem('authToken', token);\n\n      // Redireciona para a página de perfil\n      this.$router.push('/perfil');\n    } catch (error) {\n      console.error(error.response?.data?.message || 'Erro ao realizar login');\n    }\n  },\n},\n\n};\n</script>\n\n<style>\nhtml,\nbody {\n  margin: 0;\n  padding: 0;\n  width: 100%;\n  height: 100%;\n}\n\n#app {\n  width: 100%;\n  height: 100%;\n}\n</style>\n\n<style scoped>\n\n.logo {\n  max-width: 300px;\n  height: auto;\n  margin: 0 auto;\n  display: block;\n  filter: drop-shadow(4px 4px 6px rgba(0, 0, 0, 1));\n}\n\n.login-container {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: flex-start;\n  background: url('@/assets/image_fundo.jpg') no-repeat center center;\n  background-size: 90% auto;\n  background-position: center;\n  font-family: Arial, sans-serif;\n  color: #fff;\n  position: relative;\n  width: 100%;\n}\n\n.overlay {\n  position: absolute;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  background-color: rgba(0, 0, 0, 0.5);\n  z-index: 0;\n}\n\n.header {\n  background: linear-gradient(to bottom, #016126, #06DB58, #03e75b);\n  width: 100%;\n  padding: 20px 0;\n  text-align: center;\n  position: relative;\n  top: 0;\n}\n\n.header h1 {\n  margin: 0;\n  font-size: 2.5rem;\n  color: white;\n}\n\n.content {\n  display: flex;\n  flex-direction: row;\n  align-items: center;\n  justify-content: flex-start;\n  gap: 30px;\n  margin-top: 40px;\n  margin-right: 90px;\n  z-index: 1;\n}\n\n@keyframes float {\n  0% {\n    transform: translateY(0);\n  }\n\n  50% {\n    transform: translateY(-10px);\n  }\n\n  100% {\n    transform: translateY(0);\n  }}\n\n  .header h1 {\n    margin: 0;\n    font-size: 2.5rem;\n    color: white;\n  }\n\n  /* Conteúdo */\n  .content {\n    display: flex;\n    flex-direction: row;\n    align-items: center;\n    justify-content: flex-start;\n    gap: 30px;\n    margin-top: 40px;\n    margin-right: 90px;\n    z-index: 1;\n  }\n\n  /*ANIMAÇÃO DA IMAGEM DA TERRA*/\n  @keyframes float {\n    0% {\n      transform: translateY(0);\n    }\n\n    50% {\n      transform: translateY(-10px);\n    }\n\n    100% {\n      transform: translateY(0);\n    }\n  }\n\n\n  /* Globo */\n\n  .globe img {\n    border-radius: 50%;\n    width: 500px;\n    height: 500px;\n    object-fit: cover;\n    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.5);\n    animation: float 5s ease-in-out infinite;\n  }\n\n  .globe-img {\n    border-radius: 50%;\n    width: 500px;\n    height: 500px;\n    object-fit: cover;\n    animation: float 5s ease-in-out infinite;\n  }\n\n  .login-form {\n    background-color: rgba(0, 0, 0, 0.6);\n    padding: 30px;\n    border-radius: 10px;\n    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.3);\n    width: 300px;\n    z-index: 1;\n  }\n\n  .login-form h2 {\n    text-align: center;\n    margin-bottom: 20px;\n    color: #fff;\n    font-size: 1.5rem;\n  }\n\n  .form-group {\n    margin-bottom: 15px;\n    width: 285px;\n  }\n\n  .form-group label {\n    display: block;\n    margin-bottom: 5px;\n    font-size: 1rem;\n    color: #ccc;\n  }\n\n  .form-group input {\n    width: 100%;\n    padding: 10px;\n    border: none;\n    border-radius: 5px;\n    font-size: 1rem;\n    background-color: #fff;\n  }\n\n  .forgot-password {\n    display: block;\n    margin: 10px 0;\n    text-align: right;\n    font-size: 0.9rem;\n    color: #ccc;\n    text-decoration: none;\n  }\n\n  .forgot-password:hover {\n    text-decoration: underline;\n  }\n\n  /* Botão */\n  /* .btn-login {\n    width: 100%;\n    padding: 10px;\n    background-color: #28a745;\n    color: white;\n    border: none;\n    border-radius: 5px;\n    font-size: 1.1rem;\n    cursor: pointer;\n    transition: background-color 0.3s;\n  }\n  \n  .btn-login:hover {\n    background-color: #218838;\n  } */</style>\n"],"mappings":";AAgCA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACT,MAAMC,KAAKA,CAAA,EAAG;MACZ,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMP,KAAK,CAACQ,IAAI,CAAC,sCAAsC,EAAE;UACxEL,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,CAAC;QAEF,MAAM;UAAEK;QAAM,IAAIF,QAAQ,CAACL,IAAI;;QAE/B;QACAQ,YAAY,CAACC,OAAO,CAAC,WAAW,EAAEF,KAAK,CAAC;;QAExC;QACA,IAAI,CAACG,OAAO,CAACC,IAAI,CAAC,SAAS,CAAC;MAC9B,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAACA,KAAK,CAACP,QAAQ,EAAEL,IAAI,EAAEc,OAAM,IAAK,wBAAwB,CAAC;MAC1E;IACF;EACF;AAEA,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}